	+b.FOOTER.footer&attributes(attributes)
		+e.container.container
			+e.row.row
				.col-12.col-md
					+e.menu
						+mnu()
				//-.col-6.col-md-3.col-base-2
					+e.menu
						ul
							li: a(href="#") Об АСОГО
							li: a(href="#") Новости
							li: a(href="#") Обучение
							li: a(href="#") Новости
				//-.col-6.col-md-3.col-base-2
					+e.menu
						ul
							li: a(href="#") Пресс-центр
							li: a(href="#") Членство
							li: a(href="#") Сертификация
